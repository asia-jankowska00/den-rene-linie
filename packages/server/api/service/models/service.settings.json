{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "name": "service"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "task_list": {
      "collection": "task",
      "via": "services",
      "dominant": true
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
